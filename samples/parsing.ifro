let iter = \list ->
           @list
           list
           (\_ -> right ())
           (\str rest ->
            let res = @option
                      (f.ofString str)
                      (\_ -> substr str 2 3)
                      (\f -> f.toInt f |> i.toString)
            in
            let _ =  concat res "\n" |> write in
            left rest)
in
readLine () |> split ", " |> loop iter
